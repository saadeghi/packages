    "redefine",
    "simple",
    "pure",
    "powerful",
    "hexo-theme"
  ],
  "license": "AGPL-3.0",
  "homepage": "https://github.com/EvanNotFound/hexo-theme-redefine#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvanNotFound/hexo-theme-redefine.git"
  },
  "authors": [
    {
      "name": "EvanNotFound"
    }
  ],
  "autoupdate": {
    "source": "npm",
